{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "types": ["chrome"],
    "importHelpers": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "stripInternal": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "newLine": "LF",
    "rootDir": ".",
    "outDir": "../../bld/background"
  }
}
